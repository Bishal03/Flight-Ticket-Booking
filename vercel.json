{
    "version": 2,
    "builds": [
        {
            "src": "capstone/wsgi.py",
            "use": "@vercel/python"
            
        },
        {
            "src": "builds_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles_build" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "capstone/wsgi.py"
        }
    ]
}
